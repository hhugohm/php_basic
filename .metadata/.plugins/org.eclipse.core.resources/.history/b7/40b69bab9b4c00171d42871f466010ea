<h2>Fibonacci sequence</h2>
<?php
$iteraciones=10;
$num1=0;
$num2=1;
echo "<table>
	<tr>
		<td>Sequence#  </td>
		<td>Value      </td>
	</tr>";
for($i=2;$i<= $iteraciones;$i++){
	
echo "<tr>
		<td>$i</td>
		<td>$num2</td>
	</tr>";
}

echo "</table>";



?>

